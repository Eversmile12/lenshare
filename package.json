{
  "name": "lenshare",
  "displayName": "Lenshare",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "vitto rivabella",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@lens-protocol/widgets-react": "^0.0.4",
    "@plasmohq/messaging": "^0.1.6",
    "@plasmohq/storage": "^1.2.2",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "omit-deep": "^0.3.0",
    "plasmo": "0.66.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "twitter-api-v2": "^1.14.2",
    "viem": "^0.1.6",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "@zlib"
        ],
        "matches": [
          "https://twitter.com/*"
        ]
      }
    ]
  }
}
